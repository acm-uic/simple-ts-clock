{
  "name": "simple-js-clock",
  "version": "2.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "clean": "rm -rf ./dist/",
    "build": "npm run build-ts && npm run build-webpack",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "build-webpack": "webpack-cli -p",
    "watch-webpack": "webpack-cli -w -p",
    "watch-node": "nodemon ./dist/server.js",
    "start": "npm run clean && npm run build && node ./dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Webpack,TypeScript,Node\" -c \"magenta.bold,cyan.bold,green.bold\" \"npm run watch-webpack\" \"npm run watch-ts\" \"npm run watch-node\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bmiddha/simple-js-clock.git"
  },
  "author": "Bharat Middha",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/bmiddha/simple-js-clock/issues"
  },
  "homepage": "https://github.com/bmiddha/simple-js-clock#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.3",
    "open-weather-icons": "0.0.8",
    "path": "^0.12.7",
    "pug": "^2.0.3",
    "shelljs": "^0.8.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/compression": "0.0.36",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/googlemaps": "^3.30.16",
    "@types/node": "^11.9.4",
    "@types/shelljs": "^0.8.3",
    "@types/webpack": "^4.4.31",
    "concurrently": "^4.1.0",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.0.0",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2"
  }
}
