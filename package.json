{
  "name": "simple-js-clock",
  "version": "3.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "clean": "rimraf ./dist/",
    "build": "npm run build-ts && npm run build-webpack",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "build-webpack": "webpack-cli --mode production",
    "watch-webpack": "webpack-cli -w --mode production",
    "watch-node": "nodemon ./dist/server.js",
    "start": "node ./dist/server.js",
    "watch-server": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Webpack,TypeScript,Node\" -c \"magenta.bold,cyan.bold,green.bold\" \"npm run watch-webpack\" \"npm run watch-ts\" \"npm run watch-node\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bmiddha/simple-js-clock.git"
  },
  "author": "Bharat Middha",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/bmiddha/simple-js-clock/issues"
  },
  "homepage": "https://github.com/bmiddha/simple-js-clock#readme",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.4.0",
    "apicache": "^1.6.3",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "googleapis": "^118.0.0",
    "node-fetch": "^2.6.11",
    "path": "^0.12.7",
    "pug": "^3.0.2"
  },
  "devDependencies": {
    "@types/apicache": "^1.6.2",
    "@types/body-parser": "^1.19.2",
    "@types/compression": "1.7.2",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.2",
    "@types/node-fetch": "^2.6.4",
    "@types/webpack": "^5.28.1",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "concurrently": "^8.2.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-webpack-plugin": "^4.0.1",
    "file-loader": "^6.2.0",
    "node-sass": "^9.0.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4"
  }
}
